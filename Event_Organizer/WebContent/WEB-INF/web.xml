<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <servlet>
    <servlet-name>AdminLoginServlet</servlet-name>
    <servlet-class>com.keyupdate.Admin.AdminLoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RegisterServlet</servlet-name>
    <servlet-class>com.keyupdate.owner.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EventCreation</servlet-name>
    <servlet-class>com.keyupdate.owner.EventCreation</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DownloadServletFile</servlet-name>
    <servlet-class>com.keyupdate.User.DownloadServletFile</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Ownerlogincheck</servlet-name>
    <servlet-class>com.keyupdate.owner.Ownerlogincheck</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UploadFile</servlet-name>
    <servlet-class>com.keyupdate.owner.UploadFile</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SetKeyUpdatedate</servlet-name>
    <servlet-class>com.keyupdate.owner.SetKeyUpdatedate</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UserLoginServlet</servlet-name>
    <servlet-class>com.keyupdate.User.UserLoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UserLogincheck</servlet-name>
    <servlet-class>com.keyupdate.User.UserLogincheck</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>FileUpload</servlet-name>
    <servlet-class>com.keyupdate.owner.FileUpload</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdminLoginServlet</servlet-name>
    <url-pattern>/AdminLoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegisterServlet</servlet-name>
    <url-pattern>/RegisterServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EventCreation</servlet-name>
    <url-pattern>/EventCreation</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Ownerlogincheck</servlet-name>
    <url-pattern>/Ownerlogincheck</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UploadFile</servlet-name>
    <url-pattern>/UploadFile</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SetKeyUpdatedate</servlet-name>
    <url-pattern>/SetKeyUpdatedate</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserLoginServlet</servlet-name>
    <url-pattern>/test</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserLogincheck</servlet-name>
    <url-pattern>/UserLogincheck</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DownloadServletFile</servlet-name>
    <url-pattern>/DownloadServletFile</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FileUpload</servlet-name>
    <url-pattern>/FileUpload</url-pattern>
  </servlet-mapping>
  
</web-app>